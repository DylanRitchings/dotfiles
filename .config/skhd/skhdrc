shift + alt + ctrl - h : yabai -m window --focus west
shift + alt + ctrl - l : yabai -m window --focus east
shift + alt + ctrl - j : yabai -m window --focus south
shift + alt + ctrl - k : yabai -m window --focus north


shift + alt + ctrl + cmd - h : yabai -m window --move west
shift + alt + ctrl + cmd - l : yabai -m window --move east
shift + alt + ctrl + cmd - j : yabai -m window --move south
shift + alt + ctrl + cmd - k : yabai -m window --move north

shift + alt + ctrl - a : yabai -m window --resize left:-20:0
shift + alt + ctrl - f : yabai -m window --resize right:20:0
shift + alt + ctrl - d : yabai -m window --resize top:0:-20
shift + alt + ctrl - s : yabai -m window --resize bottom:0:20


